{
  "name": "@filipjosifoski/git-security",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc",
    "compile": "tsc --pretty --noEmit",
    "deploy": "cdk deploy",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --check src/ test/",
    "prettier:fix": "prettier --write src/ test/",
    "test": "jest",
    "test:fix": "jest --updateSnapshot"
  },
  "dependencies": {
    "aws-cdk": "^2.1031.2",
    "aws-cdk-lib": "^2.222.0",
    "constructs": "^10.4.3",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "jest-cdk-snapshot": "^2.3.6",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "prettier": {
    "printWidth": 120,
    "importOrder": [
      "^@core/(.*)$",
      "^@server/(.*)$",
      "^@ui/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ]
  },
  "engines": {
    "npm": "please-use-yarn",
    "node": ">=22",
    "yarn": "~1.22.0 <2"
  }
}
